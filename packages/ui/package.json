{
  "name": "@antho/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": "./src/*.tsx"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "lint": "eslint",
    "generate:component": "turbo gen react-component",
    "typecheck": "tsc --noEmit",
    "ui:add": "pnpm dlx shadcn@latest add && prettier src --write --list-different"
  },
  "devDependencies": {
    "@antho/eslint-config": "workspace:*",
    "@antho/tsconfig": "workspace:*",
    "@turbo/gen": "^2.4.4",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  }
}
